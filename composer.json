{
  "name": "huijiewei/yii2-bootstraptoggle",
  "description": "Bootstrap Toggle is a highly flexible Bootstrap plugin that converts checkboxes into toggles.",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-bootstraptoggle.git",
  "keywords": [
    "yii2",
    "extension",
    "bootstrap",
    "toggle",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.2",
    "yiisoft/yii2": "^2.0",
    "npm-asset/bootstrap-toggle": "^2.2.2"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\bootstraptoggle\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
